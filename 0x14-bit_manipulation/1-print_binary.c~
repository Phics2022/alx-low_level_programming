#include <stdio.h>
#include "main.h"
#include <stdlib.h>
/**
 *print_binary - print binary
 *@n: number
 *
 *Return: void
 */
void print_binary(unsigned long int n)
{
unsigned long int mask = 1 << 31;
unsigned long int len;
unsigned long int lent = sizeof(unsigned long int) * 8;
for (len = lent; len > 0; len--)
{
putchar(n & mask ? '1' : '0');
n <<= 1;
}
}
