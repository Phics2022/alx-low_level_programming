#include "dog.h"
#include <stdio.h>
/**
 *new_dog - function that creates a new dog.
 *@name: name of dog
 *@age: age of dog
 *@owner: owner
 *
 *Description: crates a new struct
 */
dog_t *new_dog(char *name, float age, char *owner)
{
  dog_t *new;
  new->name = name;
  new->age = age;
  new->owner = owner;
}
